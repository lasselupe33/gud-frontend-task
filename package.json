    "type-check:watch": "npm run type-check -- --watch",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "cache:purge": "rimraf --glob '**/.turbo'; rimraf --glob '**/.cache'; rimraf --glob '**/.eslintcache'; rimraf --glob '**/.stylelintcache'; rimraf --glob '**/tsconfig.tsbuildinfo'; rimraf --glob '**/.next'",
    "postinstall": "patch-package || true"
  },
  "dependencies": {
    "@linaria/core": "^6.2.0",
    "@tanstack/react-query": "^5.59.20",
    "core-js": "^3.39.0",
